{
    "compilerOptions": {
        "target": "es6",
        "newLine": "LF",
        "module": "es6",
        "moduleResolution": "node",
        "pretty": true,
        "noImplicitAny": true,
        "esModuleInterop": true,
        "strictNullChecks": true,
        "strictPropertyInitialization": true,
        "downlevelIteration": true,
        "strict": true,
        "rootDir": "@MTRACK_GRAPH@/src",
        "typeRoots": [ "@MTRACK_GRAPH_BUILD@" ],
        "sourceMap": true,
        "incremental": true,
        "lib": ["es6","dom"],
        "baseUrl": "."
    },
    "include": ["@MTRACK_GRAPH@/src/*.ts", "@MTRACK_GRAPH@/src/model/*.ts", "@MTRACK_GRAPH@/src/graph/*.ts"]
}
